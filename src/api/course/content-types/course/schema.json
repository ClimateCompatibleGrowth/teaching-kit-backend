{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    }
  },
  "attributes": {
    "Title": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "Abstract": {
      "pluginOptions": {},
      "type": "richtext"
    },
    "CourseCreator": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "Courses"
    },
    "Lectures": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::lecture.lecture",
      "inversedBy": "Courses"
    },
    "Prerequisites": {
      "type": "component",
      "repeatable": true,
      "component": "about-the-material.prerequisite"
    },
    "Acknowledgement": {
      "type": "text"
    },
    "Level": {
      "type": "enumeration",
      "enum": [
        "Beginner",
        "Intermediate",
        "Expert"
      ]
    },
    "CiteAs": {
      "type": "text"
    },
    "LearningOutcomes": {
      "type": "component",
      "repeatable": true,
      "component": "about-the-material.learning-outcomes"
    }
  }
}
